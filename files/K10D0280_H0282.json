{
    "K10D0280_H0282:131a-0": "@131a / /rgya gar skad du /",
    "K10D0280_H0282:131a-1": "dharma aa nu smri ti /",
    "K10D0280_H0282:131a-2": "bod skad du /",
    "K10D0280_H0282:131a-3": "chos rjes su dran pa /",
    "K10D0280_H0282:131a-4": "sangs rgyas dang /",
    "K10D0280_H0282:131a-5": "byang chub sems dpa' thams cad la phyag 'tshal lo //",
    "K10D0280_H0282:131a-6": "dam pa'i chos ni legs par gsungs pa /",
    "K10D0280_H0282:131a-7": "tshangs par spyod pa /",
    "K10D0280_H0282:131a-8": "thog mar dge ba /",
    "K10D0280_H0282:131a-9": "bar du dge ba /",
    "K10D0280_H0282:131a-10": "tha mar dge ba /",
    "K10D0280_H0282:131a-11": "don bzang po /",
    "K10D0280_H0282:131a-12": "tshig 'bru bzang po /",
    "K10D0280_H0282:131a-13": "ma 'dres pa /",
    "K10D0280_H0282:131a-14": "yongs su rdzogs pa /",
    "K10D0280_H0282:131a-15": "yongs su dag pa /",
    "K10D0280_H0282:131a-16": "yongs su byang ba /",
    "K10D0280_H0282:131b-0": "@131b sangs rgyas bcom ldan 'das kyis chos 'dul ba legs par gsungs pa /",
    "K10D0280_H0282:131b-1": "yang dag par thob pa /",
    "K10D0280_H0282:131b-2": "nad med pa /",
    "K10D0280_H0282:131b-3": "dus chad pa med pa /",
    "K10D0280_H0282:131b-4": "nye bar gtod pa /",
    "K10D0280_H0282:131b-5": "'di mthong bal don yod pa /",
    "K10D0280_H0282:131b-6": "mkhas pa rnams kyis rang gis rig par bya ba /",
    "K10D0280_H0282:131b-7": "chos 'dul ba legs par bstan pa /",
    "K10D0280_H0282:131b-8": "nges par 'byung ba /",
    "K10D0280_H0282:131b-9": "rdzogs pa'i byang chub tu 'gro bar byed pa /",
    "K10D0280_H0282:131b-10": "mi mthun pa med cing mthun pa dang ldan pa /",
    "K10D0280_H0282:131b-11": "rten pa dang ldan pa'o //",
    "K10D0280_H0282:131b-12": "rgyun lam chad pa'o //"
